{
  "name": "investment",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "node ace serve --watch",
    "debug": "node ace serve --node-args=\"--inspect\"",
    "build": "node ace build --production",
    "start:prod": "node ./build/server.js",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start_former": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "pushToGithub": "npm run build && git push -u origin dev && git push -u octa-investment-service dev",
    "test": "node ace test --watch",
    "deploy": "git add . && git commit -m Heroku && git push heroku main",
    "restart": "heroku restart -a astrapay-v2-investment",
    "heroku-postbuild": "npm install pm2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@japa/preset-adonis": "^1.0.15",
    "@japa/runner": "^2.0.7",
    "@types/source-map-support": "^0.5.4",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "typescript": "~4.6",
    "youch": "^3.1.1",
    "youch-terminal": "^2.1.3"
  },
  "dependencies": {
    "@adonisjs/ace": "^11.3.1",
    "@adonisjs/assembler": "^5.6.2",
    "@adonisjs/attachment-lite": "^1.0.7",
    "@adonisjs/bouncer": "^2.2.5",
    "@adonisjs/core": "^5.8.2",
    "@adonisjs/lucid": "^18.0.0",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.10",
    "@adonisjs/session": "^6.1.2",
    "@shagital/adonisjs-crud-generator": "^3.0.3",
    "adonis-jsonable": "^0.2.1",
    "adonis-pdf": "^0.0.22",
    "adonis-rabbit": "^0.1.2",
    "adonis5-scheduler": "^2.0.2",
    "agnese": "^0.3.0",
    "axios": "^0.27.2",
    "esm": "^3.2.25",
    "js-joda": "^1.11.0",
    "knex": "^2.0.0",
    "luxon": "^2.4.0",
    "map-anything": "^2.0.2",
    "mapper-utils": "^0.0.1",
    "moment": "^2.29.3",
    "node-cron": "^3.0.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "phc-argon2": "^1.1.3",
    "pino-datadog": "^2.0.2",
    "pino-pretty": "^9.1.1",
    "pm2": "^5.2.0",
    "proxy-addr": "^2.0.7",
    "puppeteer": "^14.1.1",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.0.8",
    "ts-node": "^10.7.0",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "^16.13.1"
  }
}
